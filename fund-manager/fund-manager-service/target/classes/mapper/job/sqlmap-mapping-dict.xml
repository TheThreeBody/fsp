<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhengtong.fsp.manager.mapper.job.dict.DictMapper" >

    <sql id="dictAttributes">
        dict_id,
        dict_name,
        from_table,
        dict_remark,
        create_time,
        update_time
    </sql>

    <resultMap id="baseResultMap" type="com.zhengtong.fsp.manager.pojo.job.dict.DictEntity">
        <id column="dict_id" property="dictId"/>
        <result column="dict_name" property="dictName" />
        <result column="from_table" property="fromTable" />
        <result column="dict_remark" property="dictRemark" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>


    <select id="selectByPrimaryKey" resultMap="baseResultMap" parameterType="java.lang.Integer" >
        select
        <include refid="dictAttributes" />
        from t_dict
        where dict_id = #{dictId}
    </select>
    <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer" >
        delete from  t_dict
        where dict_id = #{dictId}
    </delete>
    <insert id="insert" parameterType="com.zhengtong.fsp.manager.pojo.job.dict.DictEntity" >
        insert into t_dict (
         dict_id,
         dict_name,
         from_table,
         dict_remark,
         create_time,
         update_time)
         values (
         #{dictId},
         #{dictName},
         #{fromTable},
         #{dictRemark},
         #{createTime},
         #{updateTime}
      )
    </insert>
    <update id="updateByPrimaryKeySelective" parameterType="com.zhengtong.fsp.manager.pojo.job.dict.DictEntity">
        UPDATE t_dict
        <set>
            <if test="dictName != null">
                dict_name = #{dictName},
            </if>
            <if test="fromTable != null">
                from_table = #{fromTable},
            </if>
            <if test="dictRemark != null">
                dict_remark = #{dictRemark},
            </if>
            <if test="createTime != null">
                create_time = #{createTime},
            </if>
            <if test="updateTime != null">
                update_time = #{updateTime},
            </if>
        </set>
        WHERE
        dict_id = #{dictId}
    </update>
</mapper>